<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PortfolioMobile1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PortfolioMobile1"
    Shell.FlyoutBehavior="Flyout"
    Title="PortfolioMobile1">


    <FlyoutItem Title="Home" Icon="dotnet_bot.png">
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
        
    </FlyoutItem>

    <FlyoutItem Title="About" FlyoutIcon="{StaticResource IconOne}">
        <ShellContent
        Title="About"
        ContentTemplate="{DataTemplate local:AboutPage}"
        Route="AboutPage" />
    </FlyoutItem>
    
    <FlyoutItem Title="Tasks" FlyoutIcon="{StaticResource IconOne}">
        <ShellContent
    Title="Task List"
    ContentTemplate="{DataTemplate local:TaskListPage}"
    Route="TaskListPage" />

</Shell>
